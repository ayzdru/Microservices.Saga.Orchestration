{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "MassTransit": "Debug",
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "Orchestration.Service"
    }
  },  
  "AllowedHosts": "*",
  "Consul": {
    "Host": "localhost",
    "Port": 8500,
    "Scheme": "http",
    "Discovery": {
      "ServiceName": "ocelot-api-gateway-service",
      "Hostname": "localhost",
      "Port": 7227
    }
  },
  "JwtBearer": {
    "Authority": "https://localhost:44310",
    "Audience": "ocelot-api-gateway",
    "RequireHttpsMetadata": true
  }
}
